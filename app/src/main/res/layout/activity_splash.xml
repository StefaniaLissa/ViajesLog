<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/cly_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/yellow">

    <ImageView
        android:id="@+id/iv_plane"
        android:layout_width="389dp"
        android:layout_height="274dp"
        android:contentDescription="@string/viajeslog"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_plane" />

    <ImageView
        android:id="@+id/iv_logo"
        android:layout_width="285dp"
        android:layout_height="285dp"
        android:contentDescription="@string/viajeslog"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_plane"
        app:srcCompat="@drawable/ic_viajes_log" />

</androidx.constraintlayout.widget.ConstraintLayout>